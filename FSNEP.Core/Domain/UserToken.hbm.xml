<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class
	  name="FSNEP.Core.Domain.UserToken, FSNEP.Core"
	  table="UserTokens" >
		<id name="ID" access="nosetter.lowercase" column="UserToken">
			<generator class="assigned" />
		</id>

		<many-to-one name="AssociatedUser" column="UserId" class="FSNEP.Core.Domain.User, FSNEP.Core"></many-to-one>
		
	</class>
</hibernate-mapping>