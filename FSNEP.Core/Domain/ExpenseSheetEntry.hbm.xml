<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="FSNEP.Core.Domain.ExpenseSheetEntry, FSNEP.Core"
    table="ExpenseSheetEntries" >
    <id name="ID" access="nosetter.lowercase" column="ID">
      <generator class="identity" />
    </id>

    <property name="Amount" column="ExpenseAmount" />
    <property name="Comment" column="Comment" />
    <property name="Description" column="Description" />
    
    <many-to-one name="AssociatedExpenseSheet" column="ExpenseSheetID" class="FSNEP.Core.Domain.ExpenseSheet, FSNEP.Core"></many-to-one>

    <many-to-one name="Project" column="ProjectID" class="FSNEP.Core.Domain.Project, FSNEP.Core"></many-to-one>
    
    <many-to-one name="ExpenseType" column="ExpenseTypeID" class="FSNEP.Core.Domain.ExpenseType, FSNEP.Core"></many-to-one>

    <many-to-one name="Account" column="FinanceAccountID" class="FSNEP.Core.Domain.Account, FSNEP.Core"></many-to-one>

    <many-to-one name="FundType" column="FundTypeID" class="FSNEP.Core.Domain.FundType, FSNEP.Core"></many-to-one>

  </class>
</hibernate-mapping>