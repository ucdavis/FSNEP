<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class
    name="FSNEP.Core.Domain.SheetTracking, FSNEP.Core"
    table="SheetTracking" >
    <id name="ID" access="nosetter.lowercase" column="ID">
      <generator class="identity" />
    </id>

    <property name="ActionDate" column="ActionDate" />
    
    <many-to-one name="ExpenseSheet" column="ExpenseSheetID" class="FSNEP.Core.Domain.ExpenseSheet, FSNEP.Core"></many-to-one>
    
    <many-to-one name="TimeSheet" column="TimeSheetID" class="FSNEP.Core.Domain.TimeSheet, FSNEP.Core"></many-to-one>

    <many-to-one name="User" column="UserId" class="FSNEP.Core.Domain.User, FSNEP.Core"></many-to-one>
    
    <many-to-one name="Status" column="StatusID" class="FSNEP.Core.Domain.Status, FSNEP.Core"></many-to-one>

  </class>
</hibernate-mapping>